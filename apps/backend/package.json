{
  "name": "@moodflow/backend",
  "version": "1.0.0",
  "description": "",
  "author": "Emmanuelle BONOLI",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "tsx src/db/prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "prisma generate --schema=./src/db/prisma/schema.prisma",
    "db:dev": "prisma db push --force-reset --schema=./src/db/prisma/schema.prisma && prisma db seed --schema=./src/db/prisma/schema.prisma && prisma studio --schema=./src/db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./src/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@eslint/js": "^9.32.0",
    "@moodflow/types": "^1.0.0",
    "@nestjs/common": "^10.2.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.0",
    "@nestjs/platform-socket.io": "^10.2.0",
    "@nestjs/websockets": "^10.2.0",
    "@prisma/client": "^5.0.0",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "globals": "^16.3.0",
    "groq-sdk": "^0.27.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^7.1.3",
    "typescript-eslint": "^8.38.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.2.0",
    "@nestjs/testing": "^10.2.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
